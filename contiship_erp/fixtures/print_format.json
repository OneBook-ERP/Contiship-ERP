  "html": "<html lang=\"en\">\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <title>Tax Invoice â€“ Contiship Logistics Pvt Ltd</title>\r\n  <style>\r\n   \r\n    .company-logo {\r\n      max-height: 60px;\r\n    }\r\n    .company-name {\r\n      font-weight: bold;\r\n      font-size: 14px;\r\n    }\r\n   \r\n  </style>\r\n</head>\r\n<body>\r\n  <div>\r\n  <table style=\"width:100%; border:0; font-size:16px;\">\r\n      <tr>\r\n          <td style=\"text-align:center; width:80%;\">\r\n            {% if doc.docstatus == 0 %}\r\n              PROFORMA INVOICE\r\n            {% elif doc.docstatus == 1 %}\r\n              TAX INVOICE\r\n            {% elif doc.docstatus == 2 %}\r\n              TAX INVOICE\r\n            {% endif %}\r\n          </td>\r\n          <td style=\"text-align:right; width:20%;\">\r\n              {% if doc.docstatus == 0 %}\r\n                <span style=\"color: #fff; background-color: #5bc0de; padding: 4px 8px;\r\n                             font-weight: bold; border-radius: 4px;\">\r\n                  DUPLICATE\r\n                </span>\r\n              {% elif doc.docstatus == 1 %}\r\n                <span>\r\n                  ORIGINAL\r\n                </span>\r\n              {% elif doc.docstatus == 2 %}\r\n                <span style=\"color: #fff; background-color: #d9534f; padding: 4px 8px;\r\n                             font-weight: bold; border-radius: 4px;\">\r\n                  CANCELLED\r\n                </span>\r\n              {% endif %}\r\n            </td>\r\n\r\n        </tr>\r\n\r\n    </table>\r\n\r\n\r\n    <!-- Header -->\r\n    <table style=\"width: 100%;\">\r\n      <tr>\r\n        <td style=\"width: 60%; border: 1px solid #000; border-bottom:none;\">\r\n          {% set comp = frappe.get_doc(\"Company\", doc.company) %}\r\n          {% if comp.company_logo or comp.image %}\r\n            <img src=\"{{ comp.company_logo or comp.image }}\" class=\"company-logo\" />\r\n          {% endif %}\r\n          <div c